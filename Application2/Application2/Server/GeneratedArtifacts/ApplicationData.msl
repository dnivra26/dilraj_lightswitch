          <ScalarProperty Name="note" ColumnName="note" />
          <ScalarProperty Name="status" ColumnName="status" />
        </MappingFragment>
      </EntityTypeMapping>
    </EntitySetMapping>
    <EntitySetMapping Name="Loan_Pays">
      <EntityTypeMapping TypeName="IsTypeOf(LightSwitchApplication.Loan_Pay)">
        <MappingFragment StoreEntitySet="Loan_Pays">
          <ScalarProperty Name="Id" ColumnName="Id" />
          <ScalarProperty Name="RowVersion" ColumnName="RowVersion" />
          <ScalarProperty Name="amount_paid" ColumnName="amount_paid" />
          <ScalarProperty Name="paid_date" ColumnName="paid_date" />
          <ScalarProperty Name="Customer_Loan_Pay" ColumnName="Customer_Loan_Pay" />
        </MappingFragment>
      </EntityTypeMapping>
    </EntitySetMapping>
  </EntityContainerMapping>
</Mapping>